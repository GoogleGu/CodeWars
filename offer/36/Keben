import java.util.Set;
import java.util.HashSet;
public class Solution {
     public static ListNode FindFirstCommonNode(ListNode pHead1, ListNode pHead2) {
       Set<Integer> list = new HashSet();
        while (pHead1 != null) {
            list.add(pHead1.val);
            pHead1 = pHead1.next;
        }

        while (pHead2 != null) {
            if (!list.add(pHead2.val)) {
                return pHead2;
            }
            pHead2 = pHead2.next;
        }
        return null;
    }
    
}
